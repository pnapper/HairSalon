<h1>Client list for @Model["stylist"].GetName()</h1>
<body>
  @if (@Model["client"].Count == 0)
  {
    <h3>No clients have been added yet!</h3>
  }
  <table>
    <tr>
      <th>Id</th>
      <th>Client</th>
    </tr>
    @foreach (var client in @Model["client"])
    {
      <h4>@client.GetId() | <a href="/clients/@client.GetId()/edit">Edit</a></h4>

      <tr>
      <td>@client.GetId()</td>
      <td>@client.GetName() | <a href="/@client.GetName()/@client.GetId()/client/delete">Delete</a></td>
      </tr>
    }
  </table>
<br>
  <a href="/@Model["stylist"].GetName()/@Model["stylist"].GetId()/client/add">Add Client</a>
  <a href="/">Homepage</a>
</body>
